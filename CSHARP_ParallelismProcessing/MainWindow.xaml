<Window x:Class="CSHARP_ParallelismProcessing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSHARP_ParallelismProcessing"
        mc:Ignorable="d"
        Title="CSHARP_ParallelismProcessing" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btn_nomal"
                Click="btn_nomal_Click"
                Content="일반"/>
        <Button x:Name="btn_ParallelismProcessing"
                Click="btn_ParallelismProcessing_Click"
                Content="병렬 처리"
                Grid.Column="1"/>
    </Grid>
</Window>
